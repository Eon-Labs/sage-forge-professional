{
  "git.ignoredRepositories": [
    "repos/nautilus_trader",
    "repos/finplot", 
    "repos/claude-flow"
  ],
  "git.detectSubmodules": false,
  "git.repositoryScanMaxDepth": 1,
  "git.openRepositoryInParentFolders": "never",
  "scm.repositories.visible": 1,
  "git.alwaysShowStagedChangesResourceGroup": false,
  "git.showInlineOpenFileAction": false,
  "scm.defaultViewMode": "list",
  "scm.alwaysShowProviders": false,
  "scm.providers.visible": 1,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.git/logs/**": true,
    "**/.git/refs/**": true,
    "**/.git/hooks/**": true,
    "**/node_modules/**": true,
    "**/__pycache__/**": true,
    "**/.pytest_cache/**": true,
    "**/.mypy_cache/**": true,
    "**/.ruff_cache/**": true,
    "**/.venv/**": true,
    "**/venv/**": true,
    "**/env/**": true,
    "**/.env/**": true,
    "**/.virtualenv/**": true,
    "**/virtualenv/**": true,
    "**/python-env/**": true,
    "**/py-env/**": true,
    "**/.python-version": true,
    "**/.conda/**": true,
    "**/.uv-cache/**": true,
    "**/.pixi/**": true,
    "**/.cache/**": true,
    "**/.tmp/**": true,
    "**/tmp/**": true,
    "**/*.log": true,
    "**/*.tmp": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.coverage": true,
    "**/htmlcov/**": true,
    "**/.tox/**": true,
    "**/.nox/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/*.egg-info/**": true,
    "**/.ipynb_checkpoints/**": true,
    "**/.jupyter/**": true,
    "**/site-packages/**": true,
    "**/lib/python*/site-packages/**": true,
    "**/repos/nautilus_trader/**": true,
    "**/repos/finplot/**": true,
    "**/repos/claude-flow/**": true,
    "**/repos/data-source-manager/**": true,
    "**/repos/tirex/**": true,
    "**/repos/alphaforge/**": true,
    "**/trade_logs/**": true,
    "**/session_logs/**": true,
    "**/results/**": true,
    "**/data_cache/**": true,
    "**/milestones/archives/**": true,
    "**/*.egg-info/**": true,
    "**/*.csv": true,
    "**/*.parquet": true
  }
}